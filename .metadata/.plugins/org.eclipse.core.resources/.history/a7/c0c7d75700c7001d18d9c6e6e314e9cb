package sec4;

public class JavaWebEx1 {

	public static void main(String[] args) {
		javaWeb j1 = new JavaWeb();
		javaWeb j2 = new JavaWeb();
		javaWeb j3 = new JavaWeb();
		j1.dm = "김철수";
		j2.dm = "박민수";
		j3.dm = "김정적";
		
		j1.partName = "c언어";
		j2.partName = "python";
		j3.partName = "java";
		
		System.out.println(j1.dm+j1.partName);
		System.out.println(j2.dm+j2.partName);
		System.out.println(j3.dm+j3.partName);

	}

}
