package sec2;

public class Food {
	private String name;
	private int quantity;
	private int price;
	
	//생성자 함수
	public Food(){}
	public Food(String name){
		this.name = name;
	}
	public Food(String name, int quantity){
		this.name = name;
		this.quantity = quantity;
	}
	public Food(String name, int quantity, int price){
		this.name = name;
		this.quantity = quantity;
		this.price = price;
	}
}
