package sec2;

public class Example2 {

	public static void main(String[] args) {
		//문자열 길이
		String data = "김기태는 천사";
		int len = data.length();
		System.out.println("data의 글자수 : "+len);
		
		//문자열 변경
		data = "김기태는 천재 너는 멍청이";
		System.out.println("data : "+data);
		//문자열 일부를 변경
		data = data.replace("김기태", "나");
		System.out.println("data : "+data);

	}

}
