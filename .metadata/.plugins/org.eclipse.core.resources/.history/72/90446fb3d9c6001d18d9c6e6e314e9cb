package sec1;

public class CarEx1 {

	public static void main(String[] args) {
		Car car1 = new Car();
		
		car1.setCompany("현대");
		car1.setModel("아반떼");
		car1.setColor("White");
		System.out.println(car1.toString());
		
		Body body1 = new Body();
		body1.setStyle("세단");
		body1.setBumper("솔리드");
		body1.setFrendar("투썹");
		car1.setBody(body1);
		System.out.println(body1.toString());
		

	}

}
