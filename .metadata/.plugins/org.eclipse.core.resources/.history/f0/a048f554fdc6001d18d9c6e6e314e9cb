package sec3;

public class ShapeEx1 {

	public static void main(String[] args) {
		Shape one = new Shape();
		
		one.setShapeType(0);
		one.setLength(10);
		
		Shape tr = new Shape();
		
		tr.setShapeType(3);
		tr.setHeight(7);
		tr.setLength(10);
		
		System.out.println("원의 넓이 : "+one.shapeArea(10));
	}

}
