package sec3;

public class Student {
	private String name;
	private int age;
	private int pg;
	private int db;
	private int ui;
	private int tot;
	private double avg;
	
	public Student() { this("무명학생"); }
	public Student(String name) { this(name, 42); }
	public Student(String name, int age) { this(name, age, 100); }
	public Student(String name, int age, int pg){ this(name, age, pg, 80); }
	public Student(String name, int age, int pg, int db){ this(name, age, pg, db, 90); }
	public Student(String name, int age, int pg, int db, int ui){ 
		this.name = name;
		this.age = age;
		this.pg = pg;
		this.db = db;
		this.ui = ui;
	}
}
